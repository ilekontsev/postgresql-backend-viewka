{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": [
        "standard",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-use-before-define": "off",
        "prettier/prettier": "error",
        "no-unused-vars": "off",
        "camelcase": "off"
    }
}